@model WMS.Models.Site

@{
    ViewBag.Title = "Details";
}
<style>  
 dd,dt {
    margin-bottom: 5px;
}
</style>
<div>
      <form class ="form-inline" role="form">
          <div class="form-group "> <h3>Site Details</h3> </div>
      </form>  
    </div>
 <hr />
<div>
    <dl class="dl-horizontal">
        <dt>
           Site Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SiteName)
        </dd>

        <dt>
            City Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.City.CityName)
        </dd>
        <dd>
            <p></p>
             @Html.ActionLink(" Edit ", "Edit", new { id = Model.SiteID } , new { @class = "btn btn-warning" })
             @Html.ActionLink("Back to List", "Index",null,new { @class = "btn btn-primary " })
        </dd>
    </dl>
</div>